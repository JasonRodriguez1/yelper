<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <div id="star-rating"></div>
    <%= f.input :comment %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
<script>
    $('#star-rating').raty({
      path: 'https://s3-us-west-2.amazonaws.com/yelper/stars',
      scoreName: 'review[rating]'
  });
</script>
